{
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#fefefe",
                    "background": "#292d52",
                    "properties": {
                        "prefix": "   ",
                        "home_icon": "~",
                        "folder_separator_icon": " \uE0b1 ",
                        "style": "agnoster_short",
                        "max_depth": 5
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#fefefe",
                    "background": "#773918",
                    "properties": {
                        "display_status": false,
                        "display_stash_count": false,
                        "display_upstream_icon": false
                    }
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#ffffff",
                    "background": "#6CA35E",
                    "properties": {
                        "prefix": " \uF898 ",
                        "display_version": true
                    }
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#111111",
                    "background": "#c29b00",
                    "properties": {
                        "prefix": " \uE235 ",
                        "display_version": true,
                        "display_mode": "files",
                        "display_virtual_env": false
                    }
                },
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#111111",
                    "background": "#ffff66",
                    "properties": {
                        "root_icon": ""
                    }
                },
                {
                    "type": "exit",
                    "style": "diamond",
                    "foreground": "#ffffff",
                    "background": "#267b7e",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_exit_code": false,
                        "always_enabled": true,
                        "error_color": "#8f0a29",
                        "color_background": true,
                        "prefix": " "
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#fefefe",
                    "background": "#4d445f",
                    "properties": {
                        "always_enabled": true,
                        "prefix": "<#4d445f,transparent>\uE0B6</>  ",
                        "postfix": " <transparent,#4d445f>\uE0B2</>"
                    }
                },
                {
                    "type": "time",
                    "style": "diamond",
                    "invert_powerline": true,
                    "leading_diamond": "\uE0B2",
                    "trailing_diamond": "\uE0B4",
                    "background": "#1a5456",
                    "foreground": "#fefefe"
                }
            ]
        },
        {
          "type": "prompt",
          "alignment": "left",
          "newline": true,
          "segments": [
            {
              "type": "text",
              "style": "plain",
              "foreground": "#b60c33",
              "properties": {
                "prefix": "",
                "text": "\u279C"
              }
            }
          ]
        }
    ],
    "final_space": true,
    "console_title": true,
    "console_title_style": "path"
}